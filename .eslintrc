{
  "extends": ["airbnb-base"],
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["."]
      }
    }
  },
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": ["error", "never"],
    "no-use-before-define": "off",
    "arrow-parens": ["error", "as-needed"],
    "import/prefer-default-export": "off",
    "no-prototype-builtins": "off",
    "no-plusplus": "off"
  }
}
